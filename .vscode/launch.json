  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Prisma Seed",
      "type": "node",
      "request": "launch",
      "runtimeArgs": [
        "--nolazy",
        "-r",
        "ts-node/register",
        "./prisma/seed.ts"
      ],
      "sourceMaps": true,
      "cwd": "${workspaceFolder}",
      "protocol": "inspector"
    }
  ]
}
